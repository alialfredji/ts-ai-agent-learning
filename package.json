{
  "name": "typescript-ai-agents-curriculum",
  "version": "2.0.0",
  "description": "Production-grade TypeScript AI Agents curriculum with infrastructure templates",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "tsx watch",
    "build": "tsc",
    "test": "vitest --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@google/genai": "^1.25.0",
    "@langchain/anthropic": "^1.0.0",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.1",
    "@langchain/google-genai": "^1.0.0",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "ai": "^5.0.76",
    "convex": "^1.28.0",
    "langchain": "^1.0.1",
    "langsmith": "^0.3.74",
    "openai": "^6.5.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.8.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "ai",
    "agents",
    "typescript",
    "openai",
    "anthropic",
    "google",
    "langchain",
    "langgraph",
    "curriculum"
  ],
  "license": "MIT"
}
